{
    "name": "symfony/ai-models-dev-platform",
    "description": "Models.dev platform bridge for Symfony AI",
    "license": "MIT",
    "type": "symfony-ai-platform",
    "keywords": [
        "ai",
        "bridge",
        "models-dev",
        "platform"
    ],
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/ai-generic-platform": "^0.4",
        "symfony/ai-platform": "^0.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^11.5.53",
        "symfony/ai-anthropic-platform": "^0.4",
        "symfony/ai-bedrock-platform": "^0.4",
        "symfony/ai-gemini-platform": "^0.4",
        "symfony/ai-meta-platform": "^0.4",
        "symfony/ai-vertex-ai-platform": "^0.4"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Symfony\\AI\\Platform\\Bridge\\ModelsDev\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\AI\\PHPStan\\": "../../../../../.phpstan/",
            "Symfony\\AI\\Platform\\Bridge\\ModelsDev\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "thanks": {
            "name": "symfony/ai",
            "url": "https://github.com/symfony/ai"
        }
    }
}
